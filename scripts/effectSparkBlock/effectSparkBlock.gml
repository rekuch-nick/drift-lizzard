function effectSparkBlock(a, b, c, note){
	
	effectSpark(a + 4, b + 4, c);
	effectSpark(a + 4, b + 16, c);
	effectSpark(a + 4, b + 28, c);
	effectSpark(a + 16, b + 4, c);
	effectSpark(a + 16, b + 16, c);
	effectSpark(a + 16, b + 28, c);
	effectSpark(a + 28, b + 4, c);
	effectSpark(a + 28, b + 16, c);
	effectSpark(a + 28, b + 28, c);
	
	
	
}